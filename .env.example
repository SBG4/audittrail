# AuditTrail Environment Configuration
# Copy to .env and customize before deployment.

# Database credentials
DB_USER=audittrail
DB_PASSWORD=audittrail

# JWT secret key -- CHANGE THIS IN PRODUCTION
# Generate a secure key: python3 -c "import secrets; print(secrets.token_hex(32))"
SECRET_KEY=change-me-in-production

# Application port (default: 80)
APP_PORT=80
